    ASSOCIATION cr_transaccion_de =
      /** Identifica si el registro pertenece a la información del vendedor (DE)
       */
      col_interesado_de -- {1} LADM_COL_V3_0.LADM_Nucleo.COL_Interesado;
      cr_transaccion -- {0..1} CR_Transaccion;
    END cr_transaccion_de;

    ASSOCIATION cr_transaccion_derecho =
      col_drr -- {1} LADM_COL_V3_0.LADM_Nucleo.COL_DRR;
      cr_transaccion -- {0..1} CR_Transaccion;
    END cr_transaccion_derecho;

    /** Es un tipo de restricción específico que persigue al bien independientemente de la parte interesada que respalde la acreencia.
     */
    !!@ ili2db.dispName = "Hipoteca"
    CLASS CR_Hipoteca
    EXTENDS CR_Restriccion =
    END CR_Hipoteca;

    ASSOCIATION cr_predio_copropiedad =
      predio -- {0..*} CR_Predio;
      copropiedad -<> {0..1} CR_Predio;
      !!@ ili2db.dispName = "Coeficiente"
      coeficiente : LADM_COL_V3_0.LADM_Nucleo.Fraccion;
    END cr_predio_copropiedad;

    ASSOCIATION cr_predio_tramite =
      cr_predio -- {1} CR_Predio;
      cr_tramite -- {0..*} CR_Procedimiento_Catastral_Registral;
    END cr_predio_tramite;

    ASSOCIATION cr_hipotecaDerecho =
      cr_hipoteca -- {0..*} CR_Hipoteca;
      cr_derecho -- {0..*} CR_Derecho;
    END cr_hipotecaDerecho;

  END Intercambio_Catastro_Registro;

END Modelo_Aplicacion_Intercambio_Cat_Reg_V1_0.
