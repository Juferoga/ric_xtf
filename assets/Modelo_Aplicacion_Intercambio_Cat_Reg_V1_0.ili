    !!@ ili2db.dispName = "Anotación Registro"
    CLASS CR_AnotacionRegistro
    EXTENDS CR_FuenteAdministrativa =
      /** Es el número del turno con el cual se radicó el título o documento presentado en la ORIP directamente o por medios electrónicos
       */
      !!@ ili2db.dispName = "Número de radicación"
      Numero_Radicacion : TEXT*10;
