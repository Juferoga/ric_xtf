      !!@ ili2db.dispName = "Fracción de derecho"
      Fraccion_Derecho : MANDATORY TEXT*255;
    END CR_Derecho;

    /** Clase especializada de BaUnit, que describe la unidad administrativa básica para el caso de Colombia.
     * El predio es la unidad territorial legal propia de Catastro. Esta formada por el terreno y puede o no tener construcciones asociadas.
     */
    !!@ ili2db.dispName = "Predio"
    CLASS CR_Predio
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.COL_UnidadAdministrativaBasica =
      /** Corresponde al codigo del departamento al cual pertenece el predio. Es asignado por DIVIPOLA y tiene 2 dígitos.
       */
      !!@ ili2db.dispName = "Departamento"
      Departamento : MANDATORY TEXT*2;
      /** Corresponde al código del municipio al cual pertenece el predio. Es asignado por DIVIPOLA y tiene 3 dígitos.
       */
      !!@ ili2db.dispName = "Municipio"
      Municipio : MANDATORY TEXT*3;
      /** Es un código homologado para identificar los inmuebles tanto en los sistemas de información catastral como registral.
       */
      !!@ ili2db.dispName = "NUPRE"
      NUPRE : MANDATORY TEXT*11;
      /** Circulo registral
       */
      !!@ ili2db.dispName = "Código ORIP"
      Codigo_ORIP : TEXT*3;
      /** Matricula inmobiliaria
       */
      !!@ ili2db.dispName = "Matrícula inmobiliaria"
      Matricula_Inmobiliaria : TEXT*80;
      /** Se refiere a la situación actual en la que se encuentra una matrícula inmobiliaria.
       */
      !!@ ili2db.dispName = "Estado del FMI"
      Estado_FMI : Modelo_Aplicacion_Intercambio_Cat_Reg_V1_0.CR_EstadoFMITipo;
      /** Es la fecha en la cual se abrió el Folio de matricula inmobiliaria.
       */
      !!@ ili2db.dispName = "Fecha de apertura FMI"
      Fecha_Apertura_FMI : INTERLIS.XMLDate;
      /** El texto de cabida y linderos que está consignado en el registro público de la propiedad sobre el cual se ejercen los derechos.
       */
      !!@ ili2db.dispName = "Cabida y linderos en registro"
      Cabida_Linderos_Registro : TEXT;
      /** Corresponde al dato de tipo de predio incorporado en las bases de datos registrales.
       */
      !!@ ili2db.dispName = "Tipo de predio en registro"
      Clase_Suelo_Registro : Modelo_Aplicacion_Intercambio_Cat_Reg_V1_0.CR_ClasePredioRegistroTipo;
      /** Es la matrícula y NUPRE por la cual se dio apertura al predio objeto de estudio (la madre).
       */
      !!@ ili2db.dispName = "Predios matriz en registro"
      Predios_Matriz_Registro : BAG {0..*} OF Modelo_Aplicacion_Intercambio_Cat_Reg_V1_0.Intercambio_Catastro_Registro.CR_EstructuraPredioMatrizRegistro;
      /** Las matrículas y NUPRES aperturados con base en el folio objeto de estudio (los hijos).
       */
      !!@ ili2db.dispName = "Predios segregados en registro"
      Predios_Segregados_Registro : BAG {0..*} OF Modelo_Aplicacion_Intercambio_Cat_Reg_V1_0.Intercambio_Catastro_Registro.CR_EstructuraPrediosSegregadosRegistro;
      /** Nuevo código númerico de treinta (30) dígitos, que se le asigna a cada predio y busca localizarlo inequívocamente en los documentos catastrales, según el modelo determinado por el Instituto Geográfico Agustin Codazzi.
       */
      !!@ ili2db.dispName = "Número predial"
      Numero_Predial : TEXT*30;
      /** Anterior código númerico de veinte (20) digitos, que se le asigna a cada predio y busca localizarlo inequívocamente en los documentos catastrales, según el modelo determinado por el Instituto Geográfico Agustin Codazzi.
       */
      !!@ ili2db.dispName = "Número predial anterior"
      Numero_Predial_Anterior : TEXT*20;
      /** Dirección del predio.
       */
      !!@ ili2db.dispName = "Dirección"
      Direccion : BAG {0..*} OF LADM_COL_V3_0.LADM_Nucleo.ExtDireccion;
      /** Valor catastral del predio, obtenido mediante investigación y análisis estadístico del mercado inmobiliario y la metodología de aplicación correspondiente.
       */
      !!@ ili2db.dispName = "Avalúo Catastral"
      Avaluo_Catastral : BAG {0..*} OF Modelo_Aplicacion_Intercambio_Cat_Reg_V1_0.Intercambio_Catastro_Registro.CR_EstructuraAvaluoCatastral;
      /** Puntaje de cero (0) a cien (100) que se obtiene a partir de la suma de los datos que se interrelacionan: 
       * 
       * Número predial. ---> 30
       * Matrícula Inmobiliaria. ---> 30
       * Dirección. ---> 20
       * Número de documento de identidad de propietarios. ---> 15 
       * Nombre del propietario. ---> 5
       */
      !!@ ili2db.dispName = "Puntaje de interrelación"
      Puntaje_Interrelacion : 0 .. 100;
      !!@ name = "ValidacionValoresPredioTipo"
      UNIQUE NUPRE; 
      MANDATORY CONSTRAINT Tipo <> #Ordenamiento_Territorial ;
      MANDATORY CONSTRAINT Tipo <> #Servicios_Publicos ;
      MANDATORY CONSTRAINT Tipo <> #Reservas_Naturales ;
      MANDATORY CONSTRAINT Tipo <> #Parques_Naturales ;
      MANDATORY CONSTRAINT Tipo <> #Amenazas_Riesgos ;
      MANDATORY CONSTRAINT Tipo <> #Servidumbre ;
      MANDATORY CONSTRAINT Tipo <> #Superficies_Agua ;
      MANDATORY CONSTRAINT Tipo <> #Transporte ;
    END CR_Predio;

